+package com.example.machineTest.activity
+
+import android.content.Intent
+import androidx.appcompat.app.AppCompatActivity
+import android.os.Bundle
+import android.util.Log
+import androidx.recyclerview.widget.GridLayoutManager
+import com.example.machineTest.databinding.ActivityMainBinding
+import com.example.machineTest.model.Book
+import com.android.volley.Request
+import com.android.volley.RequestQueue
+import com.android.volley.toolbox.JsonObjectRequest
+import com.android.volley.toolbox.Volley
+import com.example.machineTest.adapters.BookAdapter
+import com.google.gson.Gson
+
+class MainActivity : AppCompatActivity() {
+
+    private lateinit var binding: ActivityMainBinding
+    private lateinit var bookList: ArrayList<Book>
+    private lateinit var requestQueue: RequestQueue
+    private lateinit var bookListAdapter: BookAdapter
+
+    override fun onCreate(savedInstanceState: Bundle?) {
+        super.onCreate(savedInstanceState)
+        binding = ActivityMainBinding.inflate(layoutInflater)
+        setContentView(binding.root)
+
+        initRequestQueue()
+        setupRecyclerView()
+        fetchBookData()
+    }
+
+    private fun initRequestQueue() {
+        requestQueue = Volley.newRequestQueue(this)
+    }
+
+    private fun fetchBookData() {
+        val request = JsonObjectRequest(
+            Request.Method.GET,
+            "https://api.itbook.store/1.0/new",
+            null,
+            { response ->
+
+                val gson = Gson()
+                val booksArray = gson.fromJson(
+                    response.getJSONArray("books").toString(),
+                    Array<Book>::class.java
+                )
+                bookList.addAll(booksArray)
+                bookListAdapter.notifyDataSetChanged()
+            },
+            { error ->
+                Log.e("tag", "Error: $error")
+            }
+        )
+        requestQueue.add(request)
+    }
+
+    private fun setupRecyclerView() {
+        bookList = ArrayList()
+        bookListAdapter = BookAdapter(bookList, this)
+        binding.rvBookList.layoutManager = GridLayoutManager(this, 2)
+        binding.rvBookList.adapter = bookListAdapter
+
+        bookListAdapter.setOnClickListener = object : BookAdapter.SetOnClickListener {
+            override fun setOnClick(position: Int) {
+                val intent = Intent(this@MainActivity, BookDetailsActivity::class.java)
+                intent.putExtra("bookInfo", bookList[position])
+                startActivity(intent)
+            }
+        }
+    }
+}
